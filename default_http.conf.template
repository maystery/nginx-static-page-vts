server {
    listen 8080;
    server_name FQDN_OR_IP;

    root /var/www/html;
    index index.html;

    access_log /var/log/nginx/access.log;
    error_log /var/log/nginx/error.log;

    location / {
        try_files $uri $uri/ /index.html;
    }

	location /status {
		vhost_traffic_status_display;
		vhost_traffic_status_display_format html;
	}

}
